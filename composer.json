{
    "name": "dhananjay-vaidya/s3-connector",
    "description": "A lightweight Laravel service for easy integration with the S3 Connector API. Provides simple interface to interact with S3 storage through the S3 Connector API without direct AWS SDK integration.",
    "version": "1.1.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhananjay Vaidya",
            "email": "dhananjay.vaidya@gmail.com",
            "homepage": "https://github.com/dhananjay-vaidya",
            "role": "Developer"
        }
    ],
    "keywords": [
        "laravel",
        "s3",
        "aws",
        "storage",
        "file-management",
        "api",
        "connector",
        "service"
    ],
    "homepage": "https://github.com/dhananjay-vaidya/s3-connector",
    "support": {
        "issues": "https://github.com/dhananjay-vaidya/s3-connector/issues",
        "source": "https://github.com/dhananjay-vaidya/s3-connector",
        "docs": "https://github.com/dhananjay-vaidya/s3-connector/blob/main/README.md"
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DhananjayVaidya\\S3Connector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DhananjayVaidya\\S3Connector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DhananjayVaidya\\S3Connector\\S3ConnectorServiceProvider"
            ],
            "aliases": {
                "S3Connector": "DhananjayVaidya\\S3Connector\\Facades\\S3Connector"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
